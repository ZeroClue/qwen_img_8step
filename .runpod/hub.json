{
  "name": "qwen-image-8step",
  "version": "1.0.0",
  "description": "Ultra-fast 8-step Qwen image generation with Lightning optimization. Generate high-quality 1328x1328 images in 2-4 seconds using advanced Qwen Vision-Language models with Lightning LoRA acceleration.",
  "category": "image-generation",
  "tags": [
    "qwen",
    "image-generation",
    "8steps",
    "lightning",
    "comfyui",
    "fast-generation",
    "vision-language",
    "chinese-style"
  ],

  "build": {
    "dockerfile": "./Dockerfile",
    "context": ".",
    "base_image": "runpod/worker-comfyui:5.5.0-base"
  },

  "deploy": {
    "gpu": "A40",
    "gpu_memory": "24GB",
    "cpu_cores": 8,
    "memory": "16GB",
    "disk_space": "50GB",
    "ports": ["8188:8188"],
    "env": [
      {
        "name": "COMFYUI_HOST",
        "value": "0.0.0.0"
      },
      {
        "name": "COMFYUI_PORT",
        "value": "8188"
      },
      {
        "name": "CUDA_VISIBLE_DEVICES",
        "value": "0"
      }
    ]
  },

  "models": [
    {
      "name": "Qwen-Image",
      "version": "1.0",
      "type": "diffusion",
      "description": "Qwen Vision-Language diffusion model",
      "provider": "Comfy-Org",
      "license": "Apache-2.0",
      "filename": "qwen_image_fp8_e4m3fn.safetensors"
    },
    {
      "name": "Qwen-2.5-VL-7B",
      "version": "7B",
      "type": "text-encoder",
      "description": "Qwen 2.5 Vision Language model for text understanding",
      "provider": "Comfy-Org",
      "license": "Apache-2.0",
      "filename": "qwen_2.5_vl_7b_fp8_scaled.safetensors"
    },
    {
      "name": "Qwen-Image-VAE",
      "version": "1.0",
      "type": "vae",
      "description": "Variational Autoencoder for Qwen Image",
      "provider": "Comfy-Org",
      "license": "Apache-2.0",
      "filename": "qwen_image_vae.safetensors"
    },
    {
      "name": "Qwen-Image-Lightning",
      "version": "1.0",
      "type": "lora",
      "description": "8-step Lightning LoRA for ultra-fast generation",
      "provider": "LightX2V",
      "license": "Apache-2.0",
      "filename": "Qwen-Image-Lightning-8steps-V1.0.safetensors"
    }
  ],

  "performance": {
    "generation_resolution": "1328x1328",
    "generation_steps": 8,
    "estimated_time_per_image": "2-4 seconds",
    "sampler": "euler",
    "scheduler": "simple",
    "cfg_scale": 1.0,
    "lora_strength": 3.1,
    "minimum_gpu_memory": "16GB"
  },

  "api": {
    "type": "comfyui",
    "endpoint": "/prompt",
    "protocol": "http",
    "port": 8188
  },

  "health_check": {
    "endpoint": "/system_stats",
    "interval": 30,
    "timeout": 10,
    "retries": 3
  },

  "pricing": {
    "model": "per-second",
    "base_rate": 0.0003,
    "description": "Ultra-fast 8-step generation reduces costs by 70% vs standard 20-step models"
  },

  "quality": {
    "resolution": "1328x1328",
    "image_format": "PNG",
    "color_depth": "24-bit",
    "quality_score": "85%+ compared to 20-step generations"
  },

  "compatibility": {
    "frameworks": ["comfyui"],
    "input_formats": ["json"],
    "output_formats": ["png", "jpg"]
  },

  "license": "Apache-2.0",
  "commercial_use": true,
  "attribution_required": false,

  "support": {
    "documentation": "https://runpod.io/hub/qwen-image-8step",
    "examples": "https://github.com/zeroclue/qwen_img_8step/examples",
    "issues": "https://github.com/zeroclue/qwen_img_8step/issues"
  }
}