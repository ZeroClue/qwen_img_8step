{
  "title": "Qwen Image 8-Step Generation",
  "description": "Ultra-fast 8-step AI image generation using Qwen Vision-Language models with Lightning optimization. Generate high-quality 1328Ã—1328 images in 2-4 seconds.",
  "type": "serverless",
  "category": "image",
  "config": {
    "runsOn": "GPU",
    "containerDiskInGb": 50,
    "gpuCount": 1,
    "env": [
      {
        "key": "PROMPT",
        "input": {
          "type": "string",
          "description": "Text prompt for image generation",
          "default": "A vibrant neon-lit street scene in Hong Kong"
        }
      },
      {
        "key": "SEED",
        "input": {
          "type": "number",
          "description": "Random seed for generation (use -1 for random)",
          "default": -1
        }
      },
      {
        "key": "WIDTH",
        "input": {
          "type": "number",
          "description": "Image width in pixels",
          "default": 1328,
          "min": 512,
          "max": 2048
        }
      },
      {
        "key": "HEIGHT",
        "input": {
          "type": "number",
          "description": "Image height in pixels",
          "default": 1328,
          "min": 512,
          "max": 2048
        }
      },
      {
        "key": "STEPS",
        "input": {
          "type": "number",
          "description": "Number of generation steps (fixed at 8 for speed)",
          "default": 8,
          "min": 8,
          "max": 8
        }
      },
      {
        "key": "CUSTOM_WORKFLOW",
        "input": {
          "type": "string",
          "description": "Optional: Complete ComfyUI workflow JSON to override all other settings",
          "default": ""
        }
      }
    ]
  }
}